#include <iostream>
using namespace std;

int main() {
    // Define the array
    int arr[] = {10, 20, 30, 40, 50};
    int n = sizeof(arr) / sizeof(arr[0]);
    
    // Calculate the sum
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += arr[i];
    }

    // Calculate and display the average
    double average = (double)sum / n;
    cout << "Average value of the array elements: " << average << endl;

    return 0;
}
